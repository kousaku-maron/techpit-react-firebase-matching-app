{
  "name": "techpit-react-firebase-matching-app",
  "version": "1.0.0",
  "main": "lib/index.js",
  "repository": "https://github.com/kousaku-maron/techpit-react-firebase-matching-app.git",
  "author": "kousaku-maron <kousaku.maron@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "serve": "yarn build && firebase serve --only functions",
    "shell": "yarn build && firebase functions:shell",
    "start": "yarn shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint './src/**/*.{ts,tsx}'"
    ]
  },
  "dependencies": {
    "firebase-admin": "^9.0.0",
    "firebase-functions": "^3.8.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.158",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.1",
    "typescript": "^3.8.3"
  }
}
